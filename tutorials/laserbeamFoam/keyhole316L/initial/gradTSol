FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

	#includeEtc "caseDicts/setConstraintTypes"
	
	frontAndBack
	{
        type            fixedValue;
        value           uniform (0 0 0);
	}
	
    lowerWall
    {
        type            fixedValue;
        value           uniform (0 0 0);

    }
    
    atmosphere
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    leftWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    rightWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

}


