{

	#includeEtc "caseDicts/setConstraintTypes"

	frontAndBack
	{
		type 	zeroGradient;
	/*
		type 		fixedGradient;
		gradient 	uniform -50.0;
		*/
	}

    lowerWall
    {
		type 	zeroGradient;
    }

    atmosphere
    {
		type 	zeroGradient;
    }

    leftWall
    {
		type 	zeroGradient;
    }
    
    rightWall
    {
		type 	zeroGradient;
    }

}

