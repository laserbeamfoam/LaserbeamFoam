FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      PhaseFieldProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
V_incident	(0 1 0);  //(0 1 0);  //NORMALISED IN CODE

N_sub_divisions 2;

// based on the article from Otto et al. work: https://doi.org/10.2351/1.5040652

HS_a 	0.0000125; // 25 micron diameter, 125 was used as an example, smaller diameter = heat is not applied
HS_bg 	0.0;  // location on x-axis which is moving axis
HS_velocity 4.0;  // [meters/s]
HS_lg 	0.00025; // distance in Z-direction
HS_Q 	1000;

//For linear path set oscAmpX=oscAmpz=0
//For sine path set oscAmpz=0
//For circular path set amplitude and frequency in both X and Z
HS_oscAmpX 		0;
HS_oscAmpZ 		0;

HS_oscFreqX 	0;
HS_oscFreqZ 	0;


wavelength			1.064e-6;
e_num_density		5.83e29;
elec_resistivity	2.0e-6;

//Use local search: defaults to yes

//useLocalSearch yes;

//Print extra debug information: defaults to no

debug no;

Radius_Flavour	2.0; 


