FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 	0
);

regions
(
    boxToCell
    {
        box (-0.500 0.00010 -0.500) (0.500 0.00060 0.500);
        
        fieldValues
        (
    		volScalarFieldValue alpha.metal 	1
        );
    }
    
);


