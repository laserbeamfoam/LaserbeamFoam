FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      LaserProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
V_incident	(0 1 0);	//(0 1 0);//NORMALISED IN CODE

N_sub_divisions   2; // more rays = slower, mag of 1 125 sec of calc

HS_a 	0.000050; // laser beam radius in mm 140 um
HS_bg 	0.0;
HS_velocity 	0.0;  // 0.01667 m/s is 1.0 m/min speeds
HS_lg 	0.0002; // distance in Z-direction
HS_Q 	156; // 156 Watt power

//For linear path set oscAmpX=oscAmpz=0
//For sine path set oscAmpz=0
//For circular path set amplitude and frequency in both X and Z
HS_oscAmpX 		0;
HS_oscAmpZ 		0;

HS_oscFreqX		0;
HS_oscFreqZ		0;

wavelength			1.064e-6;
e_num_density		2.83e29;
elec_resistivity	0.45e-6; // 0.45e-6 is optimized

// Use local search: defaults to yes
useLocalSearch    true; //false;

debug no;

Radius_Flavour	2.0;


