FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application		laserbeamFoam; // or the name of your solver

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.001800;

deltaT          1.0e-8;

writeControl    adjustableRunTime;

//writeInterval   0.000020; // works well, dont capture keyhole oscillations well
writeInterval   0.000050; // slow but captures well keyhole oscillations, correct

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.8; // optimized Courant number
maxAlphaCo      0.8;

maxDeltaT       5.0e-5;


// ************************************************************************* //
